<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="#FFF0F0F0">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/detail_title_banner"
        android:padding="1dp"
        android:orientation="horizontal"
        android:id="@+id/linearLayout">
        <Button
            android:id="@+id/libray_btn_toggle"
            android:layout_width="13dp"
            android:layout_height="22dp"
            android:background="@drawable/detail_btn_back"
            android:layout_marginLeft="16.47dp"
            android:layout_marginTop="13dp" />

        <com.ourincheon.wazap.NotoTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout"
        android:background="#c9c9c9"></LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/titleLayout"
        android:layout_below="@+id/linearLayout5"
        android:background="#ffffff"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="3.33dp">

        <com.ourincheon.wazap.NotoTextView
            android:id="@+id/jTitle"
            android:hint="title"
            android:textSize="17dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textStyle="bold"
            android:textColor="#464646"/>

        <com.ourincheon.wazap.NotoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="제목"
            android:id="@+id/textView40"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="12dp"
            android:textColor="#737373" />
    </FrameLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/titleLayout"
        android:id="@+id/lineardivider"
        android:background="#c9c9c9"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_below="@+id/lineardivider"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout7"
        android:layout_marginRight="4dp"
        android:layout_marginLeft="4dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="124dp"
            android:layout_height="match_parent"
            android:background="#ffffff">

            <com.ourincheon.wazap.NotoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="공모전 명칭"
                android:id="@+id/textView18"
                android:layout_gravity="left|center_horizontal"
                android:layout_marginTop="26.33dp"
                android:textSize="13dp"
                android:layout_marginLeft="12dp" />

            <com.ourincheon.wazap.NotoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="카테고리"
                android:id="@+id/textView19"
                android:layout_gravity="left|center_horizontal"
                android:layout_marginTop="18.67dp"
                android:layout_marginLeft="12dp" />

            <com.ourincheon.wazap.NotoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="모집인원"
                android:id="@+id/textView20"
                android:layout_gravity="left|center_horizontal"
                android:layout_marginTop="50dp"
                android:layout_marginLeft="12dp" />

            <com.ourincheon.wazap.NotoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="모집마감일"
                android:id="@+id/textView25"
                android:layout_gravity="left|center_horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="12dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:weightSum="1">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/editText9"
                android:layout_marginRight="12dp"
                android:hint="레드닷 어워즈"
                android:layout_marginTop="15dp"
                android:textSize="13dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginTop="5dp">

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="IT 콘텐츠"
                    android:id="@+id/checkBox4"
                    android:checked="false"
                    android:textSize="13dp" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="마케팅 광고"
                    android:id="@+id/checkBox2"
                    android:textSize="13dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="모집 인원을 선택하세요"
                    android:id="@+id/textView24"
                    android:textSize="14dp" />

            </LinearLayout>

            <com.ourincheon.wazap.NotoTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2016 / 01 / 25"
                android:id="@+id/textView34"
                android:layout_marginTop="20dp"
                android:textSize="14dp" />

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_below="@+id/linearLayout7"
        android:id="@+id/lineardivider1"
        android:background="#c9c9c9"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/lineardivider1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ffffff"
        android:layout_marginLeft="3.33dp"
        android:layout_marginRight="3.33dp"
        android:layout_marginTop="3.33dp"
        android:layout_above="@+id/jButton"
        android:layout_marginBottom="3.33dp">

        <com.ourincheon.wazap.NotoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="상세소개"
            android:id="@+id/textView7"
            android:layout_marginTop="22dp"
            android:textSize="17dp"
            android:layout_alignParentTop="false"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="22dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp" />

        <com.ourincheon.wazap.NotoTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="같이 레드닷 어워즈 함께할 팀원을 구합니다 파티원 구해요 같이 할분 구합니다
            같이 레드닷 어워즈 함께할 팀원을 구합니다 파티원 구해요 같이 할분 구합니다 같이 레드닷 어워즈 함께할 팀원을 구합니다 파티원 구해요 같이 할분 구합니다 같이 레드닷 어워즈 함께할 팀원을 구합니다 파티원 구해요 같이 할분 구합니다 같이 레드닷 어워즈 함께할 팀원을 구합니다 파티원 구해요 같이 할분 구합니다"
            android:id="@+id/textView15"
            android:layout_gravity="left|top"
            android:layout_below="@+id/textView7"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:textSize="13dp"
            android:textColor="#363636" />

    </RelativeLayout>

    <com.ourincheon.wazap.NotoTextView
        android:textColor="#ffffff"
        android:layout_width="match_parent"
        android:layout_height="41.33dp"
        android:background="#0056ff"
        android:text="모집글 올리기"
        android:gravity="center"
        android:textSize="20dp"
        android:id="@+id/jButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>